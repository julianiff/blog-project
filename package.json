{
  "name": "@julianiff/blog-project",
  "private": true,
  "version": "0.0.1",
  "description": "Blog template in 11ty",
  "main": "src/index.md",
  "module": "src/index.md",
  "type": "module",
  "scripts": {
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "build": "npm run build:clean && npm run build:gen",
    "build:clean": "rimraf public",
    "build:gen": "eleventy --config=.eleventy.cjs",
    "build:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "build:serve": "wds --root-dir=public --node-resolve --watch"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Julian Iff",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "clean-css": "^5.1.3",
    "es-module-shims": "^0.12.1",
    "json-to-lit-transformer": "^0.0.4",
    "lit": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@custom-elements-manifest/analyzer": "^0.3.11",
    "@esm-bundle/chai": "^4.1.5",
    "@open-wc/testing": "^2.5.33",
    "@open-wc/testing-karma": "^4.0.9",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@web/dev-server": "0.0.29",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/dev-server-rollup": "^0.2.7",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.8.0",
    "@web/test-runner": "^0.12.15",
    "@web/test-runner-mocha": "^0.3.5",
    "@web/test-runner-playwright": "^0.8.4",
    "concurrently": "^6.2.0",
    "cpx": "^1.5.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "lit-analyzer": "^1.1.10",
    "mocha": "^7.1.1",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.2",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.4",
    "web-component-analyzer": "^1.0.3"
  }
}
